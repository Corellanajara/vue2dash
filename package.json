{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-dashboard": "vue-cli-service build --target wc --name dashboard-app ./src/components/Dashboard.vue",
    "build-card": "vue-cli-service build --target wc --name card-custom-app ./src/components/CardCustom.vue",
    "build-area": "vue-cli-service build --target wc --name area-chart-app ./src/charts/AreaChart.vue",
    "build-bar": "vue-cli-service build --target wc --name bar-chart-app ./src/charts/BarChart.vue",
    "build-donut": "vue-cli-service build --target wc --name donut-chart-app ./src/charts/DonutChart.vue",
    "build-line": "vue-cli-service build --target wc --name line-chart-app ./src/charts/LineChart.vue",
    "build-line-column": "vue-cli-service build --target wc --name line-column-chart-app ./src/charts/LineColumnArea.vue",
    "build-polar": "vue-cli-service build --target wc --name polar-chart-app ./src/charts/PolarChart.vue",
    "build-progress": "vue-cli-service build --target wc --name progress-chart-app ./src/charts/ProgressChart.vue",
    "build-spark": "vue-cli-service build --target wc --name spark-chart-app ./src/charts/SparklineChart.vue",
    "build-triple": "vue-cli-service build --target wc --name triple-chart-app ./src/charts/TripleLineChart.vue",

    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "apexcharts": "^3.19.3",
    "axios": "^0.21.0",
    "vue": "^2.5.17",
    "vue-apexcharts": "^1.5.3",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.12",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.0-rc.12",
    "vue-template-compiler": "^2.5.17"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
